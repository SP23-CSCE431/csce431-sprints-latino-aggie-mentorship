<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.first_name %> <%= user.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>
  
  <p>
    <strong>Role:</strong>
    <%= user.role %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= user.year %>
  </p>

</div>
